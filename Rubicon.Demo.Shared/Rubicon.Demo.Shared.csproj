<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <IsPackable>true</IsPackable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    
    <PackageId>Rubicon.Demo.Shared</PackageId>
	  <Version>1.0.4</Version>
	  <Authors>Jarno Gruppen</Authors>
    <Company>Jarno's Company</Company>	  
    <PackageDescription>Rubicon shared demo project</PackageDescription>
    <RepositoryUrl>https://github.com/jgruppen/using-gitHub-packages-shared-dotnet</RepositoryUrl>
  </PropertyGroup>
  
</Project>
